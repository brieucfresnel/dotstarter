{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "standard",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "ignorePatterns": ["dist/"],
    "rules": {
      "no-new": 0,
      "space-before-function-paren": 0
    }
}
